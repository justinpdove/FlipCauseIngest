<apex:page Controller="bfc_vftrackingdashboard" sidebar="false">
<apex:stylesheet value="{!$Resource.RareCSS}" />
  
    <apex:dataTable styleClass="pageBlockTable" cellpadding="1" border="1" value="{!tracking}" var="t" id="list">
    <apex:column > 
     <apex:facet name="header">Stage</apex:facet>
    {!t.Display_Stage__c}
    </apex:column> 
    <apex:column >
    <apex:facet name="header">Current</apex:facet>
     <apex:outputfield value="{!t.Current__c}"/>
    </apex:column> 
    <apex:column >
    <apex:facet name="header">Complete</apex:facet>
<apex:outputfield value="{!t.Completed__c}"/>
    </apex:column> 
    <apex:column >
    <apex:facet name="header">Ended</apex:facet>
<apex:outputfield value="{!t.Ended__c}"/>
    </apex:column> 
    <apex:column >
    <apex:facet name="header">Cohort<br/>to Date</apex:facet>
 <apex:outputfield value="{!t.Cohort_to_Date__c}"/>
    </apex:column> 
     <apex:column >
    <apex:facet name="header">Target</apex:facet>
<apex:outputfield value="{!t.Target__c}"/>
    </apex:column> 
        <apex:column >
    <apex:facet name="header">Target<br/>Date</apex:facet>
<apex:outputfield value="{!t.Target_Date__c}"/>
    </apex:column> 
    </apex:dataTable>
 
</apex:page>